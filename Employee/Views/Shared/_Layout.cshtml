<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
	<link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
	<link href="~/css/site.css" rel="stylesheet" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div class="container">
        <div class="navbar-expand-sm navbar bg-dark navbar-dark border-bottom box-shadow mb-3 ">
                <img src="~/image/Kady.jpg" width="30" height="30"/>                    
            <a class="navbar-brand" asp-action="Index" asp-controller="Home">
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContents" aria-expanded="false" aria-label="Toggle navigation">
